{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "@gitackt",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "./node_modules/.bin/tsc --build tsconfig.json",
    "test": "jest --coverage",
    "watch": "jest  --watch --coverage",
    "generate": "openapi-generator generate -g typescript-axios -i ./openapi.yml -o ./src/interfaces --additional-properties=modelPropertyNaming=camelCase,supportsES6=true,withInterfaces=true,typescriptThreePlus=true",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "devDependencies": {
    "@types/faker": "^5.1.0",
    "@types/jest": "^26.0.3",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "@openapitools/openapi-generator-cli": "^2.2.6"
  },
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.17",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "firebase-admin": "^9.2.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.9.5"
  }
}
